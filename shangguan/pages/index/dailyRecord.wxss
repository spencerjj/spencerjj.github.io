page{
  background: #f9f9f9;
}
.container {
  width: 100%;
  overflow-x: hidden;
}

.title {
  font-size: 40rpx;
  color: #666;
  width: 90%;
  margin-left: 5%;
  position: relative;
}

.main {
  border-radius: 10rpx;
  border: 2rpx dashed #ccc;
  margin-top: 30rpx;
  padding: 5rpx;
  height: 80rpx;
  font-size: 40rpx;
  text-align: center;
  color: #af9d7f;
  font-weight: bolder;
  width: 90%;
  margin-left: 5%;
}

.btm {
  border-radius: 10rpx;
  border: 2rpx solid #ccc;
  margin-top: 30rpx;
  padding: 5rpx;
  height: 80rpx;
  font-size: 35rpx;
  z-index: 1;
  color: rgb(51, 51, 51)
}

.introduce {
  border-radius: 10rpx;
  border: 2rpx solid #ccc;
  margin-top: 30rpx;
  padding: 10rpx;
  font-size: 35rpx;
  padding-left: 10px;
  width: 96%;
  color: rgb(51, 51, 51)
}

.c1 {
  margin-top: 20rpx;
  position: relative
}

.c1 image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 8rpx;
  left: 20rpx
}

.c1 text {
  font-size: 30rpx;
  color: #666;
  padding-left: 10rpx;
  font-weight: bolder;
}

.codePicker {
  width: 99%;
  margin-left: 1%;
  height: 440rpx;
  color: #af9d7f;
  margin-top: 10rpx;
  position: relative;
  font-weight: bolder;
  text-align: center;
  text-align: left;
  border: 1rpx solid #ccc;
  overflow: hidden;
}

/* .cboder{
  border: 1rpx solid #ccc;
} */
.codePicker .tip {
  width: 150rpx;
  height: 150rpx;
  margin-top: 105rpx;
}

.codePicker1 {
  width: 98%;
  height: 180rpx;
  color: #af9d7f;
  padding-left: 10rpx;
  padding-top: 10rpx;
  margin-top: 10rpx;
  position: relative;
  border: 1rpx solid #ccc;
  font-size: 40rpx;
  font-weight: bolder;
  margin-left: 1%;
}
.codeHeight{
  height:380rpx !important
}
.placeholder{
  color: #af9d7f;
}

.progress {
  font-size: 35rpx;
  background: white;
  color: #af9d7f;
  border: 2rpx solid #af9d7f;
  height: 80rpx;
  line-height: 80rpx;
  width: 90%;
  margin-left: 5%;
  text-align: center;
  border-radius: 10rpx;
  margin-top: 50rpx;
  margin-bottom: 100rpx;
}

.timePicker {
  width: 97%;
  height: 80rpx;
  line-height: 80rpx;
  color: #af9d7f;
  padding-left: 10rpx;
  margin-top: 10rpx;
  position: relative;
  font-size: 40rpx;
  border-bottom: 1rpx solid #eee;
  font-weight: bolder;
}

.arraw {
  width: 20rpx;
  height: 25rpx;
  top: 50%;
  margin-top: -22.5rpx;
  right: 20rpx;
  position: absolute
}

.tip {
  width: 100%;
  text-align: right;
  font-size: 28rpx;
  color: red;
  margin-top: 20rpx;
  font-weight: bolder;
}

.codePicker {
  display: flex;
  justify-content: left;
  flex-wrap: wrap;
}
.up {
  width: 100%;
}

.codePicker .photo {
  width: 32.3%;
  height: 220rpx;
  margin-left: 1.5%;
  padding-top: 0;
  position: relative;
}

.codePicker .photo:nth-child(3n+1) {
  margin-left: 0;
}

.codePicker .photo:nth-child(n+4) {
  margin-top: 10rpx;
}

.photo image {
  width: 100%;
  height: 100%;
}

.codePicker .photo1 {
  background: #eee;
  text-align: center;
  vertical-align: middle;
}

.codePicker .photo1 image {
  width: 100rpx;
  height: 100rpx;
  margin-top: 60rpx;
}

.area{
  /* border-bottom:15rpx solid #eee; */
  padding-bottom: 30rpx;
  height: 130rpx;
  overflow: hidden;
  transition: .2s linear;
  background:white;
  margin-bottom: 15rpx;
  width: 96%;
  margin-left: 2%;
  border-radius: 10rpx;
}
.areaAll{
  /* height: 1500rpx !important; */
  height: 750rpx;
}
.area:first-child{
  border-bottom:none !important
}
.delete {
  position: absolute;
  width: 60rpx !important;
  height: 60rpx !important;
  right: 5rpx;
  top: 5rpx;
}

.red {
  color: red !important
}

.list .top{
  width: 94%;
  margin-left: 3%;
  height: 100rpx;
  line-height: 100rpx;
  background: white;
  margin-top: 30rpx;
  color: #9b9b9b;
  line-height: 100rpx;
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  border:2rpx solid #eee;
  border-radius:10rpx;
  box-shadow: 1px 1px 3px #eee;
}
.error{
  border:2rpx solid red !important
}
.list .name {
  margin-left: 10rpx;
  font-size: 30rpx;
  font-weight: bolder;
  width: 35%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  float: left;
}
.list .title{
  margin-left: 10rpx;
  font-size: 30rpx;
  font-weight: bolder;
  width: 40%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  float: left;
}
.arraw {
  width: 30rpx;
  height: 35rpx;
  top: 50%;
  margin-top: -17.5rpx;
  right: 15rpx;
  position: absolute;
  transition: 0.1s linear;
}
.active{
  color: #af9d7f;
  font-size: 40rpx;
  line-height: 100rpx;
}
.selected{
  transform: rotate(180deg);
}

.count{
  width: 40rpx;
  height: 40rpx;
  top: 50%;
  margin-top: -20rpx;
  right: 55rpx;
  position: absolute;
}
.sonLists{
  position: absolute;
  width: 99%;
  left: 0;
  max-height: 320rpx;
  z-index: 99;
  top: 146rpx;
  overflow-y: scroll;
  border: 1rpx solid #dbcaad;
  border-radius: 0 0 10rpx 10rpx;
  background: #f9f9f9;
}
.sonItem{
  height: 76rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #dbcaad;
  /* background: #f5f5f5; */
  padding-left: 20rpx;
  line-height: 76rpx;
  white-space: nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}
.sonItem:last-child{
  border-bottom: none;
}
::-webkit-scrollbar
{
width: 6px;
height: 6px;
background-color: #ffffff;
}
::-webkit-scrollbar-track
{
border-radius: 10px;
background-color: #fff;
}
::-webkit-scrollbar-thumb
{
border-radius: 10px;
background-color: #999;
}
.hover1{
  background: #ddd;
}