.container {
  width: 90%;
  margin-left: 5%;
}

.title {
  font-size: 40rpx;
  color: #666
}

.main {
  border-radius: 10rpx;
  border: 2rpx dashed #ccc;
  margin-top: 30rpx;
  padding: 5rpx;
  height: 80rpx;
  font-size: 40rpx;
  text-align: center;
  color: #af9d7f;
  font-weight: bolder;
}

.btm {
  border-radius: 10rpx;
  border: 2rpx solid #ccc;
  margin-top: 30rpx;
  padding: 5rpx;
  height: 80rpx;
  font-size: 35rpx;
  z-index: 1;
  color: rgb(51, 51, 51)
}

.introduce {
  border-radius: 10rpx;
  border: 2rpx solid #ccc;
  margin-top: 30rpx;
  padding: 10rpx;
  font-size: 35rpx;
  padding-left: 10px;
  width: 96%;
  color: rgb(51, 51, 51)
}

.c1 {
  margin-top: 20rpx;
  position: relative
}

.c1 image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 8rpx;
  left: 20rpx
}

.c1 text {
  font-size: 30rpx;
  color: #666;
  padding-left: 10rpx;
  font-weight: bolder;
}

.codePicker {
  width: 100%;
  min-height: 220rpx;
  color: #af9d7f;
  margin-top: 10rpx;
  position: relative;
  font-weight: bolder;
  text-align: center;
  text-align: left;
  border: 1rpx solid #ccc;

}

/* .cboder{
  border: 1rpx solid #ccc;
} */
.codePicker .tip {
  width: 150rpx;
  height: 150rpx;
  margin-top: 105rpx;
}

.codePicker1 {
  width: 99%;
  height: 380rpx;
  color: #af9d7f;
  padding-left: 10rpx;
  padding-top: 10rpx;
  margin-top: 10rpx;
  position: relative;
  border: 1rpx solid #ccc;
  font-size: 40rpx;
  font-weight: bolder;
}

.bottom{
  width: 92%;
  margin-left:4%;
  background:white;
  z-index: 99;
  margin-top:50rpx;
  display: flex;
  justify-content: center;
}
.publish{
  font-size: 35rpx;
  background: #af9d7f;
  color:white;
  height: 80rpx;
  line-height: 80rpx;
  
}
.progress{
  font-size: 35rpx;
  background: white;
  color:#af9d7f;
  border: 2rpx solid #af9d7f;
  height: 80rpx;
  line-height: 80rpx;
}
.btnwid{
  width: 100%;
}
.btnwid1{
  width: 40%;
}

.timePicker {
  width: 97%;
  height: 80rpx;
  line-height: 80rpx;
  color: #af9d7f;
  padding-left: 10rpx;
  margin-top: 10rpx;
  position: relative;
  font-size: 40rpx;
  border-bottom: 1rpx solid #eee;
  font-weight: bolder;
}

.arraw {
  width: 20rpx;
  height: 25rpx;
  top: 50%;
  margin-top: -22.5rpx;
  right: 20rpx;
  position: absolute
}

.tip {
  width: 100%;
  text-align: right;
  font-size: 28rpx;
  color: #69bcf9;
  margin-top: 20rpx;
  font-weight: bolder;
}

.codePicker {
  display: flex;
  justify-content: left;
  flex-wrap: wrap;
}

.up {
  width: 100%;
}

.codePicker .photo {
  width: 32.3%;
  height: 220rpx;
  margin-left: 1.5%;
  padding-top: 0;
  position: relative;
}

.codePicker .photo:nth-child(3n+1) {
  margin-left: 0;
}

.codePicker .photo:nth-child(n+4) {
  margin-top: 10rpx;
}

.photo image {
  width: 100%;
  height: 100%;
}

.codePicker .photo1 {
  background: #eee;
  text-align: center;
  vertical-align: middle;
}

.codePicker .photo1 image {
  width: 100rpx;
  height: 100rpx;
  margin-top: 60rpx;
}

.delete {
  position: absolute;
  width: 40rpx !important;
  height: 40rpx !important;
  right: 5rpx;
  top: 5rpx;
}

.red {
  color: red !important
}
.proShow{
  height: 100%;
  width: 100%;
  background: #f7f7f7;
}
.lists{
  position: relative; 
  padding-left: 20rpx;
  padding-top:10rpx;
  clear: both;
  padding-bottom: 20rpx;
  z-index: 3;
  background: white;
  border-radius: 20rpx;
  width: 98%;
  margin:0 auto;
  margin-bottom: 30rpx;
  border: 2rpx solid #f3f3f3;
  box-sizing: border-box;
  box-shadow: 0 0 5px #ccc;
  margin-left: -20rpx;
  top:-50rpx
}
.right{
  width: 100%;
}
.time1{
  font-size: 30rpx;
  line-height: 35rpx;
  padding-top:15rpx;
  font-weight: bolder;
  color:#69bcf9
}
.time1 text{
  display: inline-block;
}
.timeLeft{
  width: 30%;
  color:#666;
  float: left;
}
.timeRight{
  width: 65%;
}