page {
  background: #f7f7f7
}

.list {
  width: 94%;
  margin-left: 3%;
  height: 100rpx;
  background: white;
  margin-top: 30rpx;
  color: black;
  line-height: 100rpx;
  border-radius: 10rpx;
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-shadow: 0 0 5rpx #ccc;
}
.hover1{
  background:#eee
}
.active{
  color: #5cadff;
}
.expired{
  color:#9b9b9b
}
.list .date {
  font-size: 32rpx;
  font-weight: bolder;
}

.list .name {
  margin-left: 15rpx;
  font-size: 32rpx;
  font-weight: bolder;
  min-width: 40%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  float: left;
}

.call {
  width: 40rpx;
  height: 45rpx;
  top: 50%;
  margin-top: -22.5rpx;
  right: 20rpx;
  position: absolute
}
.nullDataInfo{
  width: 100%;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  font-size: 30rpx;
  color: #ddd;
  padding-top: 50rpx;
  padding-bottom:50rpx
}