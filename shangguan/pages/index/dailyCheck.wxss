.container {
  width: 100%;
  overflow-x: hidden;
}

.title {
  font-size: 40rpx;
  color: #666;
  width: 90%;
  margin-left: 5%;
}

.main {
  border-radius: 10rpx;
  border: 2rpx dashed #ccc;
  margin-top: 30rpx;
  padding: 5rpx;
  height: 80rpx;
  font-size: 40rpx;
  text-align: center;
  color: #af9d7f;
  font-weight: bolder;
  width: 90%;
  margin-left: 5%;
}

.btm {
  border-radius: 10rpx;
  border: 2rpx solid #ccc;
  margin-top: 30rpx;
  padding: 5rpx;
  height: 80rpx;
  font-size: 35rpx;
  z-index: 1;
  color: rgb(51, 51, 51)
}

.introduce {
  border-radius: 10rpx;
  border: 2rpx solid #ccc;
  margin-top: 30rpx;
  padding: 10rpx;
  font-size: 35rpx;
  padding-left: 10px;
  width: 96%;
  color: rgb(51, 51, 51)
}

.c1 {
  margin-top: 20rpx;
  position: relative
}

.c1 image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 8rpx;
  left: 20rpx
}

.c1 text {
  font-size: 30rpx;
  color: #666;
  padding-left: 10rpx;
  font-weight: bolder;
}

.codePicker {
  width: 100%;
  height: 440rpx;
  color: #af9d7f;
  margin-top: 10rpx;
  position: relative;
  font-weight: bolder;
  text-align: center;
  text-align: left;
  border: 1rpx solid #ccc;
  overflow: hidden;
}

/* .cboder{
  border: 1rpx solid #ccc;
} */
.codePicker .tip {
  width: 150rpx;
  height: 150rpx;
  margin-top: 105rpx;
}

.codePicker1 {
  width: 99%;
  height: 380rpx;
  color: #af9d7f;
  padding-left: 10rpx;
  padding-top: 10rpx;
  margin-top: 10rpx;
  position: relative;
  border: 1rpx solid #ccc;
  font-size: 35rpx;
  font-weight: bolder;
}
.placeholder{
  color: #af9d7f;
}

.progress {
  font-size: 35rpx;
  background: white;
  color: #af9d7f;
  border: 2rpx solid #af9d7f;
  height: 80rpx;
  line-height: 80rpx;
  width: 90%;
  margin-left: 5%;
  text-align: center;
  border-radius: 10rpx;
  margin-top: 50rpx;
  margin-bottom: 100rpx;
}

.timePicker {
  width: 97%;
  height: 80rpx;
  line-height: 80rpx;
  color: #af9d7f;
  padding-left: 10rpx;
  margin-top: 10rpx;
  position: relative;
  font-size: 35rpx;
  border-bottom: 1rpx solid #eee;
  font-weight: bolder;
}

.arraw {
  width: 20rpx;
  height: 25rpx;
  top: 50%;
  margin-top: -22.5rpx;
  right: 20rpx;
  position: absolute
}

.tip {
  width: 100%;
  text-align: right;
  font-size: 28rpx;
  color: #69bcf9;
  margin-top: 20rpx;
  font-weight: bolder;
}

.codePicker {
  display: flex;
  justify-content: left;
  flex-wrap: wrap;
  margin-bottom: 30rpx;
}

.up {
  width: 150rpx;
  height: 150rpx;
  margin-top: 140rpx;
  margin-left: 40%;
}


.codePicker .rightPhoto{
  width: 100%;
}

.area{
  border-bottom:15rpx solid #eee;
}
.area:first-child{
  border-bottom:none !important
}
.delete {
  position: absolute;
  width: 60rpx !important;
  height: 60rpx !important;
  right: 5rpx;
  top: 5rpx;
}

.red {
  color: red !important
}