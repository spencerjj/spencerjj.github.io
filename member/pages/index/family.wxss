.container {
  position: relative;
}

.list {
  width: 92%;
  background: #fff;
  margin: 0 auto;
  padding: 10rpx 0;
  margin-top: 30rpx;

}

.list .list-item {
  width: 90%;
  height: 130rpx;
  display: flex;
  align-items: center;
}

.list .list-item .profile-box {
  position: relative;
  width: 100rpx;
  height: 100rpx;
  margin-left: 30rpx;
}

.list .list-item .profile-box .profile {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  left: 0;
  top: 0;
  z-index: 0;
}

.list .list-item .info-box {
  position: relative;
  width: 100%;
  height: 120rpx;
  /* border: 1px #000 solid;  */
  padding: 30rpx 6rpx 10rpx 40rpx;
}

.list .list-item .info-box .name {
  font-size: 30rpx;
  font-weight: bolder;
  float: left;
  max-width: 165rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;

}

.list .list-item .info-box .title {
  font-size: 25rpx;
  margin-top: 3rpx;
  margin-left: 20rpx;
  color: white;
  width: 100rpx;
  border-radius: 25rpx;
  text-align: center;
  float: left;
}
.list .list-item .tip {
  font-size: 25rpx;
  margin-top: 3rpx;
  margin-left: 20rpx;
  color: #1790FF;
  border:1rpx solid #1790FF;
  width: 100rpx;
  border-radius: 25rpx;
  text-align: center;
  float: left;
}
.title1 {
  background: #1790FF;
}

.title2 {
  background: #FF9900
}

.list .list-item .info-box .phone {
  width: 35rpx;
  height: 35rpx;
  float: left;
  margin-left: 10rpx;
  margin-top: 3rpx
}

.list .list-item .info-box .number {
  font-size: 28rpx;
  float: left;
  color: #9ea7b4;
  margin-left: 20rpx;
}

.list .list-item .info-box .line2 {
  clear: both;
  padding-top: 20rpx;
}

.list .list-item .info-box .tag {
  margin-right: 16rpx;
}

.list .list-item .info-box .age {
  width: 40rpx;
  height: 40rpx;
  float: left;
}

.shop {
  font-size: 30rpx;
  font-weight: bolder;
  color: #1790FF;
  float: left;
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.arraw {
  top: 50%;
  margin-top: -25rpx;
  right: -50rpx;
  position: absolute;
  background-image: linear-gradient(to right, #f71b1b , #f78888);
  color:#fff;
  font-size: 25rpx;
  padding:5rpx 15rpx 5rpx 15rpx;
  border-radius: 10rpx;
  box-shadow: 0rpx 0rpx 5rpx 1rpx #bbb;
}
.active{
  background-image: linear-gradient(to right, #1790FF , #93c6f7);
}
.card {
  width: 100%;
  text-align: center;
  background:white;
  padding-bottom:100rpx
}
.section{
  width: 96%;
  padding-left: 4%;
  border-top: 1rpx solid #cac9c9;
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
}
.s1{
  float:left;
  width: 30%;
  text-align: right;
  color:#666
}
.fill1{
  float: left;
  width: 68%;
  text-align: center;
  height:90rpx;
  line-height: 90rpx;
  font-weight: bolder;
  color:#666
}
.action{
  text-align: center;
  width: 80%;
  margin-left:10%;
  height: 70rpx;
  font-size: 30rpx;
  color:white;
  line-height: 70rpx;
  border-radius: 10rpx;
  opacity: 0.5;
  margin-top:100rpx
}
.save{
  background: #096DD9;
}
.hover1{
  opacity: .8;
}
.arraw1{
  width: 30rpx;
  height: 30rpx;
  top:50%;
  margin-top: -15rpx;
  right: 30rpx;
  position: absolute;
}