.myTabbar{
  width: 100%;
  position: fixed;
  bottom:50rpx;
  z-index: 6;
}
.cover{
  width: 100%;
  height: 100%;
  position: fixed;
  top:0;
  left:0;
  z-index: 5;
  background:rgba(0,0,0,.2);
  overflow: hidden;
}
.boss{
  width: 100%;
  height: 180rpx;
  position: relative;
}
.myTabbar .mainimg{
  width: 100%;
  max-height: 170rpx;
  position: absolute;
  bottom:0;
  z-index: 6;
}
.myTabbar .main1{
  opacity: 0;
  transition: ease-in-out .5s;
}
.myTabbar .plus{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  left: 50%;
  margin-left: -13rpx;
  top:55rpx;
}
.run{
  transform: rotate(360deg);
}
.myTabbar .plus>image:first-child{
  width: 80%;
  height: 8rpx;
  position: absolute;
  top:30rpx;
  transition: linear .3s;
  z-index: 10;
}
.myTabbar .plus>image:nth-child(2){
  height: 80%;
  width: 8rpx;
  position: absolute;
  top:10rpx;
  left: 20rpx;
  transition: linear .3s;
  z-index: 10;
}
.myTabbar .mainShow{
  opacity: 1;
}
.sections{
  opacity: 0;
  transition: ease-in-out .2s;
}
.secItem image{
  width: 40rpx;
  max-height: 40rpx;
}
.secItem{
  position: absolute;
  bottom:40rpx;
  z-index: 9;
  width: 15%;
  text-align: center;
  font-size: 22rpx;
  color:#a6a6a6
}
.item1{
  left:5%
}
.item2{
  left:20%
}
.item3{
  right:20%
}
.item4{
  right:5%
}
.iconItem{
  width: 110rpx;
  height: 110rpx;
  position: absolute;
  border-radius: 50%;
  box-shadow: 0 0 20rpx 0 #ccc;
  opacity: 0;
  transition: linear .2s;
}
.iconShow{
  opacity: 1;
  transform:translateY(0) translateX(0) !important;
}
.icon1{
  left:25%;
  top:-70rpx;
  transform:translateY(120%) translateX(145%);
}
.icon2{
  left:45%;
  top:-130rpx;
  transform:translateY(155%);
}
.icon3{
  right:23%;
  top:-70rpx;
  transform:translateY(120%) translateX(-145%);
}