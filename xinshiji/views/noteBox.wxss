.check{
  width: 100%;
  text-align: right;
  font-size: 25rpx;
  color:#666;
  padding-top:40rpx
}
.note-box{
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.note-box>view{
  width: 48%;
  border-radius: 20rpx;
  overflow: hidden;
  position: relative;
  background:white
}
.note-box>view:nth-child(4n+1){
  height: 400rpx;
  margin-top:20rpx
}
.note-box>view:nth-child(4n+1) image{
  width: 100%;
  height: 300rpx;
}
.note-box>view:nth-child(4n+2){
  height: 300rpx;
  margin-top:20rpx
}
.note-box>view:nth-child(4n+2) image{
  width: 100%;
  height: 200rpx;
}
.note-box>view:nth-child(4n+3){
  height: 300rpx;
  margin-top: 20rpx;
}
.note-box>view:nth-child(4n+3) image{
  width: 100%;
  height: 200rpx;
}
.note-box>view:nth-child(4n+4){
  height: 400rpx;
  margin-top: -80rpx;
}
.note-box>view:nth-child(4n+4) image{
  width: 100%;
  height: 300rpx;
}
.boxTitle{
  width: 90%;
  margin:0 auto;
  font-size: 28rpx;
  height: 40rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.create{
  display: flex;
  justify-content: space-between;
  width: 90%;
  margin:0 auto;
  font-size: 25rpx;
  color:#999
}
.create>view:first-child image{
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
}
.create>view{
  display: flex;
  align-items: center;
}