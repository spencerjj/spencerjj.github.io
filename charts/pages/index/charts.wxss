.title {
	text-align: center;
	position: relative;
	margin-top: 20rpx;
	margin-bottom: 20rpx;
}
.back-btn {
	position: absolute;
	left: 20rpx;
	top: 0;
}
.table {
  border: 0px solid darkgray;
  margin-top: 100rpx;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 50rpx;
	font-size: 25rpx;
	line-height: 50rpx;
 }
 .td {
  width:100%;
  justify-content: center;
  text-align: center;
  overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	border:2rpx solid white
 }
 .td:nth-child(2){
	 width: 500%;
 }
 .td:last-child{
	color:red
}
 .th:nth-child(2){
	width: 500%;
}
 .bg-w{
  background: #ebeff7;
 }
 .bg-g{
  background: #d3dfef;
 }
 .th {
  width: 100%;
  justify-content: center;
  background: #3aa1ff;
  color: #fff;
  display: flex;
	height: 50rpx;
	line-height: 50rpx;
 }
 .trhover{
  background: #ddd;
}
.c1{
  position: absolute;
  top:0;
  z-index: 1;
}
.c2{
  position: absolute;
  top:0;
  z-index: 9;
}