		<view class="chartsDetail">会员销售占比</view>
		<view class="mark">单位：人数</view>
		<canvas canvas-id="areaCanvas" class="canvas" bindtouchstart="toPage" data-mark="area"></canvas>
		<view class="toDetail" bindtap="toDetail">点击查看详细数据</view>
	</view>
	<view style="width:100%;height:15rpx;background:#eee"></view>
