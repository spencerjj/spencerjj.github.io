	<view class="topCon {{ifUp?'topHide':''}}">
		<view class="first">
			<view class="title">数据发生时间：</view>
			<view class="time">
				<!-- <view class="date">
					10/29
				</view> -->
				<picker mode="date" fields="day" value="{{date}}" start="{{thisYear}}-01-01" end="{{yestoday}}" bindchange="bindDateChange">
				<view class="date">
					{{showDate}}
					<image class="arraw1" src="/images/arraw.png"></image>
				</view>
			</picker>
				<view class="fullDate">
					<view>{{today}}</view>
					<view>星期{{week}}</view>
				</view>
			</view>
		</view>
		<view class="middle">
			<view class="mfirst">
				<view class="msection">
					<view class="mtop">
						<text class="mainTitle">会员人数</text>
					</view>
					<view class="mdata">
						{{totalVip}}
					</view>
					<view class="mtime">
						本年度
					</view>
				</view>
				<view class="msection">
					<view class="mtop">
						<text class="mtitle">{{thisYear}}新招人数</text>
						<text class="mcompare  {{(vipCardCot2020-vipCardCot2019)>0?'rise':'down'}}">{{vipCardCot2020>vipCardCot2019?'+':'-'}}{{vipCardCot2020-vipCardCot2019}}</text>
					</view>
					<view class="mdata">
						{{vipCardCot2020}}
					</view>
					<view class="mtime">
						本年度
					</view>
				</view>
				<view class="msection">
					<view class="mtop">
						<text class="mtitle">{{lastYear}}新招人数</text>
						<text class="mcompare">0</text>
					</view>
					<view class="mdata">
						{{vipCardCot2019}}
					</view>
					<view class="mtime">
						去年
					</view>
				</view>
			</view>
			<view class="mfirst" style="margin-top:40rpx">
				<view class="msection">
					<view class="mtop">
						<text class="mtitle">招新转化率</text>
						<text class="mcompare {{(vipRate2020-vipRate2019)>0?'rise':'down'}}">{{vipRate2020>vipRate2019?'+':''}}{{(vipRate2020-vipRate2019)}}.00%</text>
					</view>
					<view class="mdata">
						{{vipRate2020}}%
					</view>
					<view class="mtime">
						本年度
					</view>
				</view>
				<view class="msection">
					<view class="mtop">
						<text class="mtitle">招新复购率</text>
						<text class="mcompare {{(reRate2020-reRate2019)>0?'rise':'down'}}">{{reRate2020>reRate2019?'+':''}}{{(reRate2020-reRate2019)}}.00%</text>
					</view>
					<view class="mdata">
						{{reRate2020}}%
					</view>
					<view class="mtime">
						本年度
					</view>
				</view>
				<view class="msection">
					<view class="mtop">
						<text class="mtitle">老会员回购率</text>
						<text class="mcompare {{(oldRate2020-oldRate2019)>0?'rise':'down'}}">{{oldRate2020>oldRate2019?'+':'-'}}{{(oldRate2020-oldRate2019)}}.00%</text>
					</view>
					<view class="mdata">
						{{oldRate2020}}%
					</view>
					<view class="mtime">
						本年度
					</view>
				</view>
			</view>
		</view>
