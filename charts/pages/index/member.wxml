		<view class="trend2020">
			<view class="chartsDetail">{{thisYear}}销售趋势</view>
			<view class="mark">单位：人数</view>
			<canvas canvas-id="columnCanvas2020" class="canvas c1"></canvas>
			<canvas canvas-id="lineCanvas2020" disable-scroll="true" class="canvas c2" bindtouchstart="touchHandler2020"></canvas>
		</view>
		<view class="trend2019">
			<view class="chartsDetail">{{lastYear}}销售趋势</view>
			<view class="mark">单位：人数</view>
			<canvas canvas-id="columnCanvas2019" class="canvas c1"></canvas>
			<canvas canvas-id="lineCanvas2019" disable-scroll="true" class="canvas c2" bindtouchstart="touchHandler2019"></canvas>
		</view>
