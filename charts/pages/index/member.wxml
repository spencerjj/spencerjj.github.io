		<view class="chartsTitle">
			实收占比<text>　　全部</text>
		</view>
		<view class="chartsDetail">{{thisYear}}实收占比</view>
		<canvas canvas-id="pieCanvas2020" class="canvas" style="height:300px"></canvas>
		<view class="chartsDetail">{{lastYear}}实收占比</view>
		<canvas canvas-id="pieCanvas2019" class="canvas" style="height:300px"></canvas>
	</view>
