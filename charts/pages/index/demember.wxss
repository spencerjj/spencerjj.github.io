page{
  background-color: #f8f8f8;
}
.container{
  width:100%;
  position: relative;
}
.select{
  position: fixed;
  left: 30rpx;
  bottom: 30rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  border:#37a1df 1px solid;
  box-shadow: 0 0 15rpx 0 #37a1df;
  background: rgba(49, 59, 109, 0.8);
  z-index: 999;
  text-align: center;
}
.select image{
  width: 80%;
  height: 80%;
  margin-top:10%
}
.selectHover{
  background: #37a1df;
}
.second{
  width: 94%;
  margin: auto
}
.comItem{
  display: flex;
  background:rgba(49, 59, 109, 0.8);
  width: 94%;
  margin:auto;
  height: 250rpx;
  border-radius: 20rpx;
  margin-top:30rpx;
  position: relative;
  box-shadow: 0 0 15rpx 0rpx #313b6dcc;
}
.fleft{
  width: 25%;
}
.fIcon{
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  background:#464d85;
  text-align: center;
  margin:70rpx auto
}
.fIcon image{
  width: 50%;
  height: 50%;
  margin-top:25%
}
.fright{
  width: 70%;
}
.fnew{
  width: 30%;
}
.newSection{
  color:white;
  text-align: center;
}
.newSection:first-child{
  margin-top:45rpx
}
.newSection:nth-child(2){
  margin-top:25rpx
}
.newSection view:nth-child(2){
  font-size:25rpx;
}
.newSection view:nth-child(1){
  font-size: 40rpx;
  font-weight: bolder;
}
.intime{
  display: flex;
  justify-content: space-around;
  height: 150rpx;
}
.comTitle{
  line-height: 150rpx;
}
.sales{
  margin-top:20rpx
}
.comTitle,.sales{
  text-align: center;
  width: 45%;
  border-radius: 10rpx;
}
.sales view{
  font-size:25rpx;
  font-weight: 500;
}
.sales{
  color:white;
  font-weight: bolder;
  font-size: 60rpx;
}
.saleTop{
  display: flex;
  justify-content: space-around;
  color:#eee;
}
.saleSection{
  text-align: center;
  height: 160rpx;
  width: 45%;
  position: relative;
  border-radius: 10rpx;
}
.saleSection view{
  font-size:25rpx;

}
.saleSection view:nth-child(1){
  font-size: 40rpx;
  font-weight: bolder;
}
