.progress1{
  font-size: 35rpx;
  background: white;
  background-color:#af9d7f;
  color:white;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  bottom: 30rpx;
  width: 90%;
  left: 5%;
  margin:0 auto
}
.sonLists{
  position: absolute;
  width: 94%;
  margin-left: 3%;
  max-height: 370rpx;
  background: #fff;
  z-index: 99;
  top:225rpx;
  overflow-y: scroll;
  border:1rpx solid #dbcaad;
  border-radius: 0 0 10rpx 10rpx;
}
.sonItem{
  height: 76rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #dbcaad;
  /* background: #f5f5f5; */
  padding-left: 20rpx;
  line-height: 76rpx;
  white-space: nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}
::-webkit-scrollbar
{
width: 6px;
height: 6px;
background-color: #ffffff;
}
::-webkit-scrollbar-track
{
-webkit-box-shadow: inset 0 0 10px rgba(0,0,0,0.3);
border-radius: 10px;
background-color: #fff;
}
::-webkit-scrollbar-thumb
{
border-radius: 10px;
-webkit-box-shadow: inset 0 0 10px rgba(0,0,0,.3);
background-color: #999;
}
.hover1{
  background: #ddd;
}