page {
  background: #f7f7f7
}

.list {
  width: 94%;
  margin-left: 3%;
  height: 100rpx;
  background: white;
  margin-top: 30rpx;
  color: black;
  line-height: 100rpx;
  border-radius: 10rpx;
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  box-shadow: 0 0 5rpx #ccc;
}
.hover1{
  background:#eee
}
.active{
  color: #5cadff;
}
.expired{
  color:#9b9b9b
}
.list .date {
  font-size: 26rpx;
  float:right;
  padding-right: 50rpx;
  font-weight: bolder;
}

.list .name {
  margin-left: 15rpx;
  font-size: 32rpx;
  font-weight: bolder;
  width: 50%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  float: left;
}

.arraw {
  width: 30rpx;
  height: 35rpx;
  top: 50%;
  margin-top: -17.5rpx;
  right: 15rpx;
  position: absolute
}