page{
  background:#f7f7f7
}
.canvas {
  width: 100%;
  height: 300px;
  }
  .container {
    height: 100%;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    -webkit-text-size-adjust: none;
    -webkit-user-select: none;
    font-size: 35rpx;
    color: #333;
    font-family: Helvetica,Arial,"Helvetica Neue","Droid Sans","Microsoft YaHei",sans-serif;
    padding-top: 100rpx;
  }
  .select{
    width: 94%;
    margin:0 auto;
    text-align: center;
    background:white;
    height: 80rpx;
    line-height: 80rpx;
    margin-top: 20rpx;
    box-shadow: 0 0 5rpx #ccc;
    font-size: 35rpx;
    font-weight: bolder;
    color:#5cadff
  }
  .select image{
    height: 30rpx;
    width: 30rpx;
    margin-left: 10rpx;
    margin-top: 5rpx;
  }