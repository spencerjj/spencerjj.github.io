page{
  background: #f8f8f8
}
.container{
  padding-top: 30rpx;
}
.title{
  color:#69bcf9;
  font-size: 40rpx;
  font-weight: bolder;
  text-align: center;
}
.line{
  width: 100%;
  margin-top: 30rpx;
}
.content{
  color:#555;
  font-size: 35rpx;
  display: block;
  padding-bottom: 30rpx;
}
.tdetail{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding:0 20rpx;
  background:white;
  border-bottom:2rpx solid #eee;
  font-weight: 500;
}
.tdetail>view:first-child{
  color:#999
}
.btn{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  color:white;
  background-image: linear-gradient(to right,#69bcf9,#4685b8);
  position: fixed;
  bottom: 0;
}
