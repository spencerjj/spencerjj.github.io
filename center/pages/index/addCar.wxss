.middle{
  padding-top:30rpx;
  width: 100%;
  padding:30rpx 30rpx 60rpx 30rpx;
  box-sizing: border-box;
  background:white;
}
.title{
  height: 60rpx;
  font-size: 32rpx;
  font-weight: bolder;
}
.confirm{
  width: 100%;
  height:100rpx;
  font-size: 32rpx;
  line-height: 100rpx;
  background-image: linear-gradient(to right,#f7c9ab,#c9a07e);
  margin-top: 50rpx;
  text-align: center;
  color:white;
  border-radius: 20rpx;
}
.bottom{
  width: 94%;
  margin-left:3%;
  margin-top:40rpx;
  color:#c0c0c0;
  font-size: 20rpx;
}
.bottom view:first-child{
  font-size: 30rpx;
  font-weight: bolder;
  color:#929292;
  height: 50rpx;
}
.bottom view{
  margin-top:20rpx
}